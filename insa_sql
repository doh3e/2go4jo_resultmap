--------------------------------------------------------
--  DDL for Function GET_GRADE
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "EZEN"."GET_GRADE" (p_score NUMBER, p_subject VARCHAR2) RETURN VARCHAR2 IS
    v_grade VARCHAR2(10);
BEGIN
    SELECT MIN(g.grade) INTO v_grade
    FROM ezenedu_score_grade g
    WHERE p_score >= g.svalue
    AND g.subject = p_subject;

    RETURN v_grade;
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN NULL;
END;


